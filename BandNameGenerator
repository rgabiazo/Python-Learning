
def band_name(city,pet):
    new_name = city + " " + pet
    return new_name

print("Welcome to the Band Name Generator.")
user_city = input("What's name of the city you grew up in?: ")
user_pet = input("What'your pet name?: ")

new_name = band_name(user_city,user_pet)

print(f"Your band name could be {new_name}")
